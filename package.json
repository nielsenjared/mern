{
  "name": "mern",
  "version": "1.0.0",
  "description": "Boilerplate MongoDB Express React Node app.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
