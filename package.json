  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nielsenjared/mern/issues"
  },
  "homepage": "https://github.com/nielsenjared/mern#readme",
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "mongoose": "^4.13.9",
    "path": "^0.12.7"
  }
}
